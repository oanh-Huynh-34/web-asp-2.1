@model DoAnWebBanGiay.Models.ViewModel.ProviderShoesModel
@{
    ViewData["Title"] = "Details";
}
<br />
<h2 class="text-info">Shoes Details</h2>
<div class="p-4 border rounded row">
    <div class="col-8">
       
        <div class="form-group row">
            <div class="col-4">
                <label asp-for="shoes"></label>
            </div>
            <div class="col-8">
                <input asp-for="providerShoes.Shoes.Name" readonly class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-4">
                <label asp-for="providers"></label>
            </div>
            <div class="col-8">
                <input asp-for="providerShoes.Provider.Name" readonly class="form-control" />
            </div>
        </div>
        <br />
    </div>
    <div class="form-group">
        <a asp-action="Index" class="btn btn-success">Back to List</a>
    </div>
</div>


