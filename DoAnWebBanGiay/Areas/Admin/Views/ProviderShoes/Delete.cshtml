
@model DoAnWebBanGiay.Models.ViewModel.ProviderShoesModel
@{
    ViewData["Title"] = "Delete";
}
<br />
<h2 class="text-info">Delete Provider shoe</h2>
<form method="post"  asp-antiforgery="true" enctype="multipart/form-data">
    <div class="p-4 border rounded row">
        <div class="col-8">

            <div class="form-group row">
            <div class="col-4">
                <label asp-for="shoes"></label>
            </div>
            <div class="col-8">
                <input asp-for="providerShoes.Shoes.Name" readonly class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-4">
                <label asp-for="providers"></label>
            </div>
            <div class="col-8">
                <input asp-for="providerShoes.Provider.Name" readonly class="form-control" />
            </div>
        </div>
        <br />
    </div>
        <div class="col-1"></div>
        <div class="form-group">
            <input type="submit" class="btn btn-danger" value="Delete" />
            <a asp-action="Index" class="btn btn-success">Back to List</a>
        </div>
    </div>
</form>

